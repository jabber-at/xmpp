Description: remove deps in rebar.config
 Without this patch, rebar would download and compile the listed deps
 at compile time.
Author: Philipp Huebner <debalance@debian.org>

--- a/rebar.config
+++ b/rebar.config
@@ -28,9 +28,6 @@
 
 {port_specs, [{"priv/lib/jid.so", ["c_src/jid.c"]}]}.
 
-{deps, [{fast_xml, ".*", {git, "https://github.com/processone/fast_xml", {tag, "1.1.23"}}},
-        {stringprep, ".*", {git, "https://github.com/processone/stringprep", {tag, "1.0.9"}}},
-        {p1_utils, ".*", {git, "https://github.com/processone/p1_utils", {tag, "1.0.9"}}}]}.
 
 {clean_files, ["c_src/jid.gcda", "c_src/jid.gcno"]}.
 
